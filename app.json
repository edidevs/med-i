{
  "expo": {
    "name": "Med-i",
    "slug": "med-i",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icons/icon.png",
    "splash": {
      "image": "./assets/Images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uk.co.powerh.medi"
    },
    "android": {
      "package": "uk.co.powerh.medi"
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "description": "Medi is a health app that lets you find the proper non-prescription medication to treat your symptoms."
  }
}